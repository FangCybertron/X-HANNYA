droneviewtop.setOnSeekBarChangeListener{
  onProgressChanged=function(view, progress)
    if progress==0 then
      text.setText("VERTICAL 0 X")
     Exec2("libsocket.so 22")
      
     elseif progress==1 then
      text.setText("VERTICAL 2 X")
     Exec2("libsocket.so 21")
      
     elseif progress==2 then
      text.setText("VERTICAL 4 X")
     Exec2("libsocket.so 20")
      
     elseif progress==3 then
      text.setText("VERTICAL 6 X")
     Exec2("libsocket.so 19")

     elseif progress==4 then
      text.setText("VERTICAL 8 X")
     Exec2("libsocket.so 18")

     elseif progress==5 then
      text.setText("VERTICAL 10 X")
     Exec2("libsocket.so 17")
     
     elseif progress==6 then
      text.setText("VERTICAL 12 X")
     Exec2("libsocket.so 16")
     
     elseif progress==7 then
      text.setText("VERTICAL 14 X")
     Exec2("libsocket.so 15")
     
     elseif progress==8 then
      text.setText("VERTICAL 16 X")
     Exec2("libsocket.so 14")
     
     elseif progress==9 then
      text.setText("VERTICAL 18 X")
     Exec2("libsocket.so 13")
     
     elseif progress==10 then
      text.setText("VERTICAL 20 X")
     Exec2("libsocket.so 12")
    end
  end
}


