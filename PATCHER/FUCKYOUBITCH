--------POP UP--------

function popUI()
  popXY=
  {
    LinearLayout,
    background="#00000000",
    layout_width="fill",
    layout_height="fill",
    Gravity="center",
    id="DialogExternal",
    {
      CardView,
      layout_width="300dp",
      layout_height="330dp",
      background="#ffffff",
      elevation="30",
      radius="26",
      id="DialogInternal",
      {
        LinearLayout,
        layout_width="300dp",
        layout_height="330dp",
        orientation="vertical",
        {
          LinearLayout,
          layout_width="300dp",
          layout_height="210dp",
          background="#ffffff",
          orientation="vertical",
          -- Gravity="center",
          {
            ImageView,
            layout_width="300dp",
            layout_height="140dp",
            layout_gravity="center",
            -- background="#ffffbc8d",
            src="https://raw.githubusercontent.com/missfangg/X-HANNYA/main/ICON/popup.jpg",
          },
          {
            TextView,
            layout_width="250dp",
            layout_height="50dp",
            layout_gravity="center",
            layout_marginTop="0dp",
            textColor="#090707",
            textSize="13sp",
            Gravity="center",
            text="THANKS FOR USING MY APPLICATION\n DON'T FORGET TO FOLLOW ME !",
            id="nr",
          },
        },
        {
          LinearLayout,
          layout_width="300dp",
          layout_height="120dp",
          orientation="vertical",
          Gravity="center",
          {
            TextView,
            layout_width="150dp",
            layout_height="35dp",
            layout_marginTop="0dp",
            textColor="#ffffff",
            textSize="15sp",
            Gravity="center",
            text="O K",
            id="allow",
          },
        },
      },
    },
  }
  pop=PopupWindow(activity)
  pop.setContentView(loadlayout(popXY))
  pop.setWidth(-1)
  pop.setHeight(-1)
  pop.setOutsideTouchable(false)
  import "android.graphics.drawable.BitmapDrawable"
  pop.setBackgroundDrawable(BitmapDrawable(loadbitmap("1.jpg")))
  pop.showAtLocation(view,Gravity.CENTER,0,500)

  allow.getPaint().setFakeBoldText(true)


  function allow.onClick()
    vibrator = activity.getSystemService(Context.VIBRATOR_SERVICE)
    vibrator.vibrate( long{100,15} ,-1)
    popHide()
  end




  import "android.view.animation.LayoutAnimationController"
  import "android.view.animation.Animation"
  import "android.view.animation.AlphaAnimation"
  import "android.view.animation.TranslateAnimation"
  import "android.view.animation.AnimationSet"

  animationSet = AnimationSet(true)
  layoutAnimationController=LayoutAnimationController(animationSet,0.2);
  layoutAnimationController.setOrder(LayoutAnimationController.ORDER_NORMAL);
  popX= AlphaAnimation(0,1);
  popX.setDuration(600);
  animationSet.addAnimation(popX);
  popY=TranslateAnimation(0, 0,activity.height,0)
  popY.setDuration(300);
  animationSet.addAnimation(popY);
  DialogExternal.setLayoutAnimation(layoutAnimationController);



  function CircleButton(view,InsideColor,radiu)
    import "android.graphics.drawable.GradientDrawable"
    drawable = GradientDrawable()
    drawable.setShape(GradientDrawable.RECTANGLE)
    drawable.setColor(InsideColor)
    drawable.setCornerRadii({radiu,radiu,radiu,radiu,radiu,radiu,radiu,radiu});
    view.setBackgroundDrawable(drawable)
  end

  CircleButton(allow,0xFF3CAAE7,360)

end



function popHide()
  import "android.view.animation.LayoutAnimationController"
  import "android.view.animation.Animation"
  import "android.view.animation.AlphaAnimation"
  import "android.view.animation.TranslateAnimation"
  import "android.view.animation.AnimationSet"
  import "android.view.animation.TranslateAnimation"

  Translate_up_down=TranslateAnimation(0, 0, 0, 1000)
  Translate_up_down.setDuration(300)
  --Translate_up_down.setFillAfter(true)
  DialogInternal.startAnimation(Translate_up_down)

  import "android.view.animation.Animation$AnimationListener"
  Translate_up_down.setAnimationListener(AnimationListener{
    onAnimationEnd=function()
      pop.dismiss()
    end
  })
end


