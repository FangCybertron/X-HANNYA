  date = os.date("%Y%m%d")
  if date >= Date then
    AlertDialog.Builder(this).setTitle("            ⚠ EXPIRED ⚠").setCancelable(false).setMessage("PLEASE CONTACT DEVELOPER TO GET UPDATE !!").setPositiveButton("Exit", {
      onClick = function(A0_21)
        activity.finish()
      end
    }).setNeutralButton("WHATSAPP", {
      onClick = function(A0_22)
        url = "https://wa.me/+6289631956023"
        activity.startActivity(Intent(Intent.ACTION_VIEW, Uri.parse(url)))
        activity.finish()
      end
    }).show()
    return
  end


-----MEMORY----------


function Exec(one)
 local two=activity.getApplicationInfo().nativeLibraryDir.."/"..(one)
if RootUtil().haveRoot()==true then
  os.execute("su -c chmod 777 "..two)
  Runtime.getRuntime().exec("su -c "..two)
     else
  os.execute("chmod 777 "..two)
  Runtime.getRuntime().exec(""..two)
 end
end


